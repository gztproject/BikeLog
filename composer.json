{
	"type" : "project",
	"license" : "proprietary",
	"require" : {
		"php": "^7.4",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"ramsey/uuid": "^4.0",
		"ramsey/uuid-doctrine": "^1.6",
		"sensio/framework-extra-bundle": "^5.5",
		"symfony/console": "5.0.*",
		"symfony/dependency-injection": "5.0.*",
		"symfony/dotenv": "5.0.*",
		"symfony/flex": "^1.3.1",
		"symfony/framework-bundle": "5.0.*",
		"symfony/orm-pack": "^1.0",
		"symfony/security-bundle": "5.0.*",
		"symfony/security-core": "5.0.*",
		"symfony/security-csrf": "5.0.*",
		"symfony/security-guard": "5.0.*",
		"symfony/security-http": "5.0.*",
		"symfony/twig-pack": "^1.0",
		"symfony/validator": "5.0.*",
		"symfony/yaml": "5.0.*"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"paragonie/random_compat" : "2.*",
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php72" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : false,
			"require" : "5.0.*"
		}
	},
	"name" : "gztproject/bike-log",
	"require-dev" : {
		"symfony/profiler-pack" : "^1.0"
	},
	"support" : {
		"email" : "gasper.doljak@gmail.com",
		"issues" : "https://github.com/gztproject/BikeLog/issues",
		"wiki" : "https://github.com/gztproject/BikeLog/wiki",
		"source" : "https://github.com/gztproject/BikeLog"
	},
	"description" : "A web based motorcycle maintenance logging software",
	"homepage" : "https://github.com/gztproject/BikeLog",
	"authors" : [{
			"name" : "Ga≈†per Doljak",
			"email" : "gasper.doljak@gmail.com",
			"homepage" : "https://www.gzt.si",
			"role" : "Author"
		}
	]
}
